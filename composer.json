{
    "name": "zekini/livewire-crud-generator",
    "description": "This helps in scaffolding the necessary CRUD and admin panel for applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Keania Niate Eric",
            "email": "keania@zekini.com"
        },
        {
            "name": "Llewellyn du Randt",
            "email": "llewellyn@zekini.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Zekini\\CrudGenerator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zekini\\CrudGenerator\\LivewireCrudGeneratorServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zekini\\CrudGenerator\\Tests\\": "tests"
        }
    },
    "require": {
        "zekini/laravel-generics": "^1.0.5"
    }
}