{
    "name": "zekini/livewire-crud-generator",
    "description": "This helps in scafolding the necessary CRUD and admin panel for applications",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Keania Niate Eric",
        "email": "support@zekini.com"
    }],
    "minimum-stability": "dev",

    "autoload": {
        "psr-4": {
            "Zekini\\CrudGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zekini\\CrudGenerator\\LivewireCrudGeneratorServiceProvider",
                "Spatie\\Permission\\PermissionServiceProvider",
                "Livewire\\LivewireServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zekini\\CrudGenerator\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "orchestra/testbench": "6.x-dev",
        "nunomaduro/collision": "dev-stable",
        "phpunit/phpunit": "9.5.x-dev"
    },
    "require": {
        "spatie/laravel-permission": "^5.4",
        "doctrine/dbal": "^3.2",
        "livewire/livewire": "^2.7",
        "owen-it/laravel-auditing": "^12.0",
        "mediconesystems/livewire-datatables": "dev-master"
    }
}
